{
  "name": "functapp",
  "version": "0.1.0",
  "main": "function.js",
  "private": true,
  "scripts": {
    "dev": "next dev -- -p 3001",
    "build": "next build",
    "start": "next start",
    "deploy:functions": "yarn build && cross-env NODE_ENV=production firebase deploy --only functions,hosting",
    "deploy": "yarn build && cross-env NODE_ENV=production firebase deploy",
    "serve": "npm run build && firebase emulators:start --only functions,hosting",
    "shell": "npm run build && firebase functions:shell",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "cross-env": "^7.0.3",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "next": "10.0.6",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/material-ui": "^0.21.8",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "typescript": "^4.1.3"
  }
}
